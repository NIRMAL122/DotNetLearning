@if (ViewBag.ErrorTitle == null)
{
<h3>An Error Occured while processing your Request</h3>
<h5>Please contact us on getHelpFromNirmal@gmail.com</h5>
    <hr />

}
else
{
    <h1 class="text-danger">@ViewBag.ErrorTitle</h1>
    <h5 class="text-danger">@ViewBag.ErrorMsg</h5>
}




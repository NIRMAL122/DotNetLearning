@model IEnumerable<EmployeeManagement.Models.Employee>
@{
    
    ViewData["Title"] = "List of Employees";
}

<table >
    <thead>
        <tr>
            <th>ID</th>
            <th>NAME</th>
            <th>Department</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody ">
        @foreach(var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Department</td>
                <td>@emp.Email</td>
            </tr>
        }
    </tbody>
</table>
